<h1>New Event</h1>

<%= form_with model: @event do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :location %>
    <%= f.text_field :location %>

    <%= f.label :date %>
    <%= f.date_field :date %>

    <%= f.label :description %>
    <%= f.text_area :description %>    

    <%= f.submit %>
<% end %>